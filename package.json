  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bkniffler/alpacka/issues"
  },
  "homepage": "https://github.com/bkniffler/alpacka#readme",
  "dependencies": {
    "lerna": "^2.9.0"
  },
  "devDependencies": {
    "jest": "^22.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
